_G.WalkSpeed = 16
_G.JumpHeight = 85

getgenv().SpamTP = false
getgenv().RejoinSame = false
getgenv().RejoinDiff = false

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Player Teleports", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "LocalPlayer",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false})

Tab:AddButton({
	Name = "Rejoin Same Server",
	Callback = function(bool)
		getgenv().RejoinSame = bool
		if bool then
		game:GetService'TeleportService':TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService'Players'.LocalPlayer)
	end
  end    
})

Tab:AddButton({
	Name = "Rejoin Different Server",
	Callback = function(bool)
	getgenv().RejoinDiff = bool
	if bool then
		local tpservice= game:GetService("TeleportService")
		local plr = game.Players.LocalPlayer
		tpservice:Teleport(game.PlaceId, plr)
	end
end    
})

Tab:AddToggle({
	Name = "Teleport",
	Default = false,
	Callback = function(bool)
	getgenv().SpamTP = bool
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = "InsertPlayerHere"
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end
	end)
end    
})


Tab:AddSlider({
	Name = "Beta",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed Value Beta",
	Callback = function(bool)
	_G.WalkSpeed = bool
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = bool
	end
end    
})

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = 85,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed Value Beta",
	Callback = function(bool)
	_G.JumpHeight = bool
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
	end
end    
})

OrionLib:Init()
