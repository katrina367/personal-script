_G.WalkSpeed = 16
_G.JumpHeight = 85
_G.RejoinSame = false
_G.RejoinDiff = false
getgenv().Suicide = false
getgenv().SpamTP = false
getgenv().TeleportStatus = false

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "Daz Script Hub loading",
	Image = "rbxassetid://4483345998",
	Time = 30
})

wait(2)

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "LOADING.",
	Image = "rbxassetid://4483345998",
	Time = 1
})

wait(0.5)

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "LOADING..",
	Image = "rbxassetid://4483345998",
	Time = 1
})

wait(0.5)

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "LOADING...",
	Image = "rbxassetid://4483345998",
	Time = 1
})

wait(3)

OrionLib:MakeNotification({
	Name = "Notification",
	Content = "Made By daz. Special thanks to Jacko.",
	Image = "rbxassetid://4483345998",
	Time = 30
})

local Window = OrionLib:MakeWindow({Name = "Player Teleports", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Player Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "Check Console",
	Default = false,
	Callback = function(bool)
	getgenv().Suicide = bool
	if bool then
	spawn(function()
		while Suicide == bool do
			print('something amazing has happened! send this to daz')
			wait()
			end
		end)
	end
end    
})

local Section = Tab:AddSection({
	Name = "Player Options"
})

Tab:AddSlider({
	Name = "[UPDATED] Walkspeed",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Change Your Walkspeed",
	Callback = function(bool)
	_G.WalkSpeed = bool
	if bool then
		local Path = game.Players.LocalPlayer.Character.Humanoid
		local hook

		hook = hookmetamethod(game, "__index", function(self, ...)
    		if self == Path and ... == "WalkSpeed" then
        		return 16
    		end
		return hook(self,...)
		end)


		Path.WalkSpeed = bool
	end
end    
})

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = 85,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "How High You Jump",
	Callback = function(bool)
	_G.JumpHeight = bool
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
	end
end    
})

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Teleports"
})

Tab:AddToggle({
	Name = "Teleport",
	Default = false,
	Callback = function(bool)
	getgenv().SpamTP = bool
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = _G.TeleportStatus
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end)
	end
end    
})

Tab:AddTextbox({
	Name = "Player Teleport Selection:",
	Default = "Player",
	TextDisappear = true,
	Callback = function(bool)
	_G.TeleportStatus = bool	
end	  
})

--[[local tbl={}
for i,v in next,game.Players:GetPlayers() do
table.insert(tbl,v.Dropdown)
end
game.Players.PlayerRemoving:Connect(function(v)
if table.find(tbl,v.Dropdown)then
table.remove(tbl,table.find(tbl,v.Dropdown))
end
end)
game.Players.PlayerAdded:Connect(function(Value)
table.insert(tbl,v.Dropdown)
end)

Tab:AddDropdown({
	Name = "Dropdown",
	Default = "1",
	Options = {"1", "2"},
	Callback = function(Value)
		print(Value)
	end    
})

Tab:AddDropdown({
	Name = "Manual Teleport WIP",
	Default = "",
	Options = {"[PlayerName]", "[PlayerName]", "[PlayerName]",},
	Callback = function(bool)
	Dropdown:Refresh(game.Players:GetChildren())
	game.Players.PlayerAdded:Connect(function() Dropdown:Refresh(game.Players:GetChildren()) end)
	game.Players.PlayerRemoving:Connect(function() Dropdown:Refresh(game.Players:GetChildren()) end)
	end    
})
]]
local Tab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Rejoins"
})

Tab:AddButton({
	Name = "[NOT WORKING]Rejoin Same Server",
	Callback = function(bool)
		_G.RejoinSame = bool
		if bool then
		game:GetService'TeleportService':TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService'Players'.LocalPlayer)
	end
  end    
})

Tab:AddToggle({
	Name = "[UPDATED] Rejoin Same Server",
	Default = false,
	Callback = function(bool)
	_G.RejoinSame = bool
		if bool then		
			game:GetService'TeleportService':TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService'Players'.LocalPlayer)
		wait()					
	end
end    
})

--[[Tab:AddButton({
	Name = "[UPDATED]Rejoin Different Server",
	Callback = function(bool)
	_G.RejoinDiff = bool
	if bool then do
		local tpservice= game:GetService("TeleportService")
		local plr = game.Players.LocalPlayer
		tpservice:Teleport(game.PlaceId, plr)
	end
end    
})
]]
OrionLib:Init()
