getgenv().SpamTP = false

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Player Teleport Script") -- Creates the window

local b = w:CreateFolder("Player Teleports") -- Creates the folder(U will put here your buttons,etc)

b:DestroyGui()

b:Toggle("Player 1",function(bool)
    getgenv().SpamTP = bool
    print('Player 1 Teleport Function is', bool)
    if bool then
            spawn(function()
                while SpamTP == bool do
                    local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local p2 = "SussyCoolGamerDude"
                    p1.CFrame.game.Players[p2].Character.HumanoidRootPart.CFrame
                    wait()
                end
            end)
        end
end)

b:Toggle("Player 2",function(bool)
    getgenv().SpamTP = bool
    print('Teleport Function is', bool)
    if bool then
            spawn(function()
                while SpamTP == bool do
                    local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
                    local p2 = "Daz_WasTaken"
                    p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
                    wait()
                end
            end)
        end
end)
