getgenv().SpamTP = false

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Player Teleports", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "LocalPlayer",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false})

Tab:AddButton({
	Name = "Rejoin Same Server",
	Callback = function(bool)
if bool then
game:GetService'TeleportService':TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService'Players'.LocalPlayer)
      		print("button pressed")
end
  	end    
})

Tab:AddButton2({
	Name = "Rejoin Different Server",
	Callback = function(bool)
if bool then
local tpservice= game:GetService("TeleportService")
local plr = game.Players.LocalPlayer
tpservice:Teleport(game.PlaceId, plr)
      		print("button pressed")
end
  	end    
})

Tab:AddButton3({
	Name = "Player Teleport",
	Callback = function(bool)
if bool then
local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
local p2 = "InsertPlayerHere"
p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
end
  	end    
})


Tab:AddSlider({
	Name = "Beta",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed Value Beta",
	Callback = function(bool)
if bool then
game.Players.LocalPlayer.Character.WalkSpeed = bool
print(bool)
end
end    
})


OrionLib:Destroy()

OrionLib:Init()

