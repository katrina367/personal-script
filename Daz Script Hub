local Window = OrionLib:MakeWindow({Name = "Daz's Script Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Player Settings",
	Icon = "rbxassetid://9525535512",
	PremiumOnly = false
})
--[[
Tab:AddButton({
	Name = "Check if Game is Supported",
	Callback = function(Value)        
		loadstring(game:HttpGet('https://raw.githubusercontent.com/katrina367/personal-script/main/Game%20Detection'))()
  	end    
})
]]
local Section = Tab:AddSection({
	Name = "Player Options"
})


local Noclipping, Clip
local plrw = game:GetService'Players'.LocalPlayer.Character
local function NoclipLoop()
    if Clip == false    then
        for _, child in pairs(plrw:GetDescendants()) do
            if child:IsA("BasePart") and child.CanCollide == true then
                child.CanCollide = false
            end
        end
    end
end


Tab:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(toggleval)
        if toggleval then
            Clip = false
            Noclipping = game:GetService('RunService').Stepped:Connect(NoclipLoop)
          else
            Clip = true
            if Noclipping then
              Noclipping:Disconnect()
            end
          end
end    
})


Tab:AddButton({
	Name = "Player Suicide",
	Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
  	end    
})

Tab:AddSlider({
	Name = "[UPDATED] Walkspeed",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Change Your Walkspeed",
	Callback = function(bool)
	getgenv().WalkSpeed = bool
	if bool then
		local Path = game.Players.LocalPlayer.Character.Humanoid
		local hook

		hook = hookmetamethod(game, "__index", function(self, ...)
    		if self == Path and ... == "WalkSpeed" then
        		return 16
    		end
		return hook(self,...)
		end)


		Path.WalkSpeed = bool
        wait(0.1)
	end
end    
})

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "How High You Jump",
	Callback = function(bool)
	getgenv().JumpHeight = bool
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
        wait(0.1)
	end
end    
})

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://6723742952",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Teleport"
})



Tab:AddToggle({
	Name = "Teleport Player Toggle",
	Default = false,
	Callback = function(bool)
	getgenv().SpamTP = bool
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = getgenv().TeleportPlayer
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end)
	end
end    
})

local a

local dropdown = Tab:AddDropdown({
    Name = "Player Dropdown",
    Default = tbl,
    Options = tbl,
    Callback = function(v)
        a = v
        getgenv().TeleportPlayer = v
    end    
})
game:GetService"Players".PlayerAdded:Connect(function()
    local players = game:GetService"Players":GetChildren()
    local PlayerList = {}

    for i,v in pairs(players) do
        table.insert(PlayerList,v.Name)
    end
    dropdown:Refresh(PlayerList, true)
    dropdown:Set(a)
end)
game:GetService"Players".PlayerRemoving:Connect(function()
    wait(1)
    local players = game.Players:GetChildren()
    local PlayerList = {}

    for i,v in pairs(players) do
        table.insert(PlayerList,v.Name)
    end
    dropdown:Refresh(PlayerList, true)
    dropdown:Set(a)
end)
do
    local players = game:GetService"Players":GetChildren()
    local PlayerList = {}

    for i,v in pairs(players) do
        table.insert(PlayerList,v.Name)
    end
    dropdown:Refresh(PlayerList, true)
    dropdown:Set(a)
end


