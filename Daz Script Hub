getgenv().TeleportStatus = Adaptive
getgenv().TeleportPlayer = Adaptive
getgenv().Noclip = false
getgenv().jump = false
getgenv().Swim = false
getgenv().ClientGrav = Adaptive
getgenv().PrivateChat = ""

repeat wait() until game:WaitForChild"Players"
for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.Idled)) do
v:Disable()
end

