	Image = "rbxassetid://9715044824",
	Time = 30
})

local Window = OrionLib:MakeWindow({Name = "Daz's Script Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Player Settings",
	Icon = "rbxassetid://9525535512",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Options"
})

Tab:AddToggle({
	Name = "Kill Player",
	Default = false,
	Callback = function(bool)
	getgenv().Suicide = bool
	if bool then
	spawn(function()
		while Suicide == bool do
                game.Players.LocalPlayer.Character.Humanoid.Health = 0
            wait()
			end
		end)
	end
end    
})

Tab:AddToggle({
	Name = "Player Noclip",
	Default = false,
	Callback = function(bool)
	getgenv().Noclip = bool
        if bool then
            local runservice = game:GetService("RunService")

            local player = game:GetService("Players").LocalPlayer

            runservice.Stepped:Connect(function()
                for i,v in pairs(player.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = bool
                    end
                end
            end)
        end
	end    
})

Tab:AddSlider({
	Name = "[UPDATED] Walkspeed",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Change Your Walkspeed",
	Callback = function(bool)
	_G.WalkSpeed = bool
	if bool then
		local Path = game.Players.LocalPlayer.Character.Humanoid
		local hook

		hook = hookmetamethod(game, "__index", function(self, ...)
    		if self == Path and ... == "WalkSpeed" then
        		return 16
    		end
		return hook(self,...)
		end)


		Path.WalkSpeed = bool
        wait(0.1)
	end
end    
})

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "How High You Jump",
	Callback = function(bool)
	_G.JumpHeight = bool
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
        wait(0.1)
	end
end    
})

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://6723742952",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Teleports"
})

Tab:AddToggle({
	Name = "Teleport Player Toggle",
	Default = false,
	Callback = function(bool)
	getgenv().SpamTP = bool
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = _G.TeleportStatus
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end)
	end
end    
})

Tab:AddTextbox({
	Name = "Player Teleport Selection:",
	Default = "Player",
	TextDisappear = false,
	Callback = function(bool)
	_G.TeleportStatus = bool
			OrionLib:MakeNotification({
			Name = "Teleport Selection",
			Content = "Player has been Selected",
			Image = "rbxassetid://6723742952",
			Time = 5
		})
end	  
})

local Section = Tab:AddSection({
	Name = "Second Player Teleport"
})


Tab:AddToggle({
	Name = "Teleport Player Toggle 2",
	Default = false,
	Callback = function(bool)
	getgenv().SpamTP = bool
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = _G.TeleportPlayer
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end)
	end
end    
})

Tab:AddTextbox({
	Name = "Player Teleport Selection:",
	Default = "Player",
	TextDisappear = false,
	Callback = function(bool)
	_G.TeleportPlayer = bool
			OrionLib:MakeNotification({
			Name = "Teleport Selection",
			Content = "Player has been Selected",
			Image = "rbxassetid://6723742952",
			Time = 5
		})
end	  
})

