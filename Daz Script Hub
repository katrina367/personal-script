local function JSMPCLW_fake_script() -- GetKey.LocalScript 
	local script = Instance.new('LocalScript', GetKey)

	script.Parent.MouseEnter:Connect(function()
		script.Parent.UIStroke.Enabled = true
	end)
	script.Parent.MouseLeave:Connect(function()
		script.Parent.UIStroke.Enabled = false
	end)
end
coroutine.wrap(JSMPCLW_fake_script)()
local function RMEKGD_fake_script() -- DestroyUI.LocalScript 
	local script = Instance.new('LocalScript', DestroyUI)

	local player = game.Players.LocalPlayer

	script.Parent.MouseButton1Click:Connect(function()
		game.CoreGui.Key.Frame.Visible = false
	end)
end
