local function start()

-- // Welcome to Daz's Open Source Script Hub
-- // This Is Just a Fun Project. Dont Go Hate, I Have No Intention Of Advertising this.

repeat wait() until game:WaitForChild"Players"
for i,v in pairs(getconnections(game:GetService("Players").LocalPlayer.Idled)) do
v:Disable()
end

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- // Notifications

OrionLib:MakeNotification({
	Name = "Loading Script hub..",
	Content = "Project Daz is Loading",
	Image = "rbxassetid://8226141461",
	Time = 5.5
})

wait(2)

OrionLib:MakeNotification({
	Name = "Progress (1/3)",
	Content = "LOADING.",
	Image = "rbxassetid://6386003052",
	Time = 3.5
})

wait(0.5)

OrionLib:MakeNotification({
	Name = "Progress (2/3)",
	Content = "LOADING..",
	Image = "rbxassetid://6386003052",
	Time = 3
})

wait(0.5)

OrionLib:MakeNotification({
	Name = "Progress (3/3)",
	Content = "INITIATING LAUNCH..",
	Image = "rbxassetid://6386003052",
	Time = 2.5
})

wait(2)

OrionLib:MakeNotification({
	Name = "Success!",
	Content = "Made by Daz",
	Image = "rbxassetid://9715044824",
	Time = 10
})

wait(3)

-- \/\/\/\/\/\/\/\/\/
-- // Orion Window
--/\/\/\/\/\/\/\/\/\/

local Window = OrionLib:MakeWindow({Name = "Project Daz", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

-- \/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/
-- // Workspace Tab 
-- // Holds Main Local And Player Scripts
-- /\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\

local Tab = Window:MakeTab({
	Name = "Workspace",
	Icon = "rbxassetid://6034798461",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "FullBright",
	Callback = function(Value)
		game.Lighting.GlobalShadows = true
		game.Lighting.Ambient = Color3.new(1,1,1)
		game.Lighting.FogEnd = 1000000
	end    
})

local Section = Tab:AddSection({
	Name = "Player"
})

local Noclipping, Clip
local plrw = game:GetService'Players'.LocalPlayer.Character

Tab:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(toggleval)
		local function NoclipLoop()
			if Clip == false    then
				for _, child in pairs(plrw:GetDescendants()) do
					if child:IsA("BasePart") and child.CanCollide == true then
						child.CanCollide = false
					end
				end
			end
		end

        if toggleval then
            Clip = false
            Noclipping = game:GetService('RunService').Stepped:Connect(NoclipLoop)
          else
            Clip = true
            if Noclipping then
              Noclipping:Disconnect()
            end
    	end
	end    
})

local w = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
local j = game.Players.LocalPlayer.Character.Humanoid.JumpPower
local g = game.Workspace.Gravity

Tab:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 200,
	Default = w,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(bool)	
	local Path = game.Players.LocalPlayer.Character.Humanoid
	local hook
	if bool then
		Path.WalkSpeed = bool
	end
end    
})

hook = hookmetamethod(game, "__index", function(self, ...)
    if self == Path and ... == "WalkSpeed" then
        return 16
    end
return hook(self,...)
end)    

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = j,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(bool)
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
        wait(0.1)
	end
end    
})

Tab:AddSlider({
	Name = "Client Sided Gravity Change",
	Min = 0,
	Max = 400,
	Default = g,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Gravity",
	Callback = function(bool)
	ClientGrav = bool
	if bool then
		game.Workspace.Gravity = bool
	end
end    
})

--\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/
-- // Teleports Tab
-- // Has The Bring and Teleport Scripts
--/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://6723742952",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Player Teleport"
})

Tab:AddToggle({
	Name = "Teleport Toggle",
	Default = false,
	Callback = function(bool)
	if bool then
		spawn(function()
			while SpamTP == bool do
				local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
				local p2 = TeleportPlayer
				p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame				
			wait()
			end
		end)
	end
end    
})

Tab:AddToggle({
	Name = "Bypass Teleport Toggle",
	Default = false,
	Callback = function(bool)
	if bool then
		spawn(function()
			while SpamTP == bool do
                local pl = game.Players.LocalPlayer.Character.HumanoidRootPart
                local pl2 = TeleportPlayer
                local humanoid = game.Players.LocalPlayer.Character.Humanoid
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                wait(0.1)
                pl.CFrame = game.Players[pl2].Character.HumanoidRootPart.CFrame
			wait()
			end
		end)
	end
end    
})

-- // Use if You Want

--[[
Tab:AddButton({
	Name = "Teleport Back",
	Callback = function()
		local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
		local p2 = getgenv().TeleportPlayer
		local pos = p1.CFrame
		p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame		
		wait(1)
		p1.CFrame = pos
  	end    
})
]]

Tab:AddButton({
	Name = "Bring Player (NEEDS TOOL)",
	Callback = function()
        Target = TeleportPlayer

        NOW = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        game.Players.LocalPlayer.Character.Humanoid.Name = 1
        local l = game.Players.LocalPlayer.Character["1"]:Clone()
        l.Parent = game.Players.LocalPlayer.Character
        l.Name = "Humanoid"
        wait()
        game.Players.LocalPlayer.Character["1"]:Destroy()
        game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
        game.Players.LocalPlayer.Character.Animate.Disabled = true
        wait()
        game.Players.LocalPlayer.Character.Animate.Disabled = false
        game.Players.LocalPlayer.Character.Humanoid.DisplayDistanceType = "None"
        for i,v in pairs(game:GetService'Players'.LocalPlayer.Backpack:GetChildren())do
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
        end
        local function tp(player,player2)
        local char1,char2=player.Character,player2.Character
        if char1 and char2 then
        char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
        end
        end
        local function getout(player,player2)
        local char1,char2=player.Character,player2.Character
        if char1 and char2 then
        char1:MoveTo(char2.Head.Position)
        end
        end
        tp(game.Players[Target], game.Players.LocalPlayer)
        wait()
        tp(game.Players[Target], game.Players.LocalPlayer)
        wait()
        getout(game.Players.LocalPlayer, game.Players[Target])
        wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = NOW
        print('Bring Used On', TeleportPlayer)
  	end    
})

local a

local dropdown = Tab:AddDropdown({
    Name = "Player Dropdown",
    Default = tbl,
    Options = tbl,
    Callback = function(v)
        a = v
        TeleportPlayer = v
        print('Selected Player:', TeleportPlayer)
    end    
})

game:GetService"Players".PlayerAdded:Connect(function()
	local players = game:GetService"Players":GetChildren()
	local PlayerList = {}

	for i,v in pairs(players) do
		table.insert(PlayerList,v.DisplayName)
	end
	dropdown:Refresh(PlayerList, true)
	dropdown:Set(a)
end)
game:GetService"Players".PlayerRemoving:Connect(function()
	wait(1)
	local players = game.Players:GetChildren()
	local PlayerList = {}

	for i,v in pairs(players) do
		table.insert(PlayerList,v.DisplayName)
	end
	dropdown:Refresh(PlayerList, true)
	dropdown:Set(a)
end)
do
	local players = game:GetService"Players":GetChildren()
	local PlayerList = {}

	for i,v in pairs(players) do
		table.insert(PlayerList,v.DisplayName)
	end
	dropdown:Refresh(PlayerList, true)
	dropdown:Set(a)
end

