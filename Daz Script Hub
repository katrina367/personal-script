local GameTable = {
    {
        placeid = {6872265039},
        callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
        end
    }
}

local function AddGame(placeid, functioner)
    local InsertTable = {}
    InsertTable.placeid = {}
    InsertTable.placeid[1] = placeid
    InsertTable.callback = function()
        loadstring(game:HttpGet(functioner))()
    end
    table.insert(GameTable, InsertTable)
end

AddGame(9772878203, 'https://raw.githubusercontent.com/zReal-King/Raise-A-Floppa-2/main/Gui')
AddGame(142823291, 'https://raw.githubusercontent.com/fheahdythdr/legendary-train/main/Eclipsehub.lua')
AddGame(9551640993, "https://raw.githubusercontent.com/ExtremeAntonis/KeySystemUI/main/KeySystemUI-Obfuscated.lua")
AddGame(6284583030, "https://rawscripts.net/raw/Project-Meow_421")


Tab:AddButton({
	Name = "Activate External GUI",
	Callback = function()
        for i,v in pairs(GameTable) do
            if table.find(v.placeid, game.PlaceId) then
                v.callback()
            end
        end
    end
})


