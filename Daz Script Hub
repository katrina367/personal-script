local Window = OrionLib:MakeWindow({Name = "Project Daz", HidePremium = true, SaveConfig = false, ConfigFolder = "OrionTest"})

-- \/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/
-- // Workspace Tab 
-- // Holds Main Local And Player Scripts
-- /\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\/\

local Tab = Window:MakeTab({
	Name = "Workspace",
	Icon = "rbxassetid://6034798461",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "FullBright",
	Callback = function(Value)
		game.Lighting.GlobalShadows = true
		game.Lighting.Ambient = Color3.new(1,1,1)
		game.Lighting.FogEnd = 1000000
	end    
})

local Section = Tab:AddSection({
	Name = "Player"
})

local Noclipping, Clip
local plrw = game:GetService'Players'.LocalPlayer.Character

Tab:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(toggleval)
		local function NoclipLoop()
			if Clip == false    then
				for _, child in pairs(plrw:GetDescendants()) do
					if child:IsA("BasePart") and child.CanCollide == true then
						child.CanCollide = false
					end
				end
			end
		end

        if toggleval then
            Clip = false
            Noclipping = game:GetService('RunService').Stepped:Connect(NoclipLoop)
          else
            Clip = true
            if Noclipping then
              Noclipping:Disconnect()
            end
    	end
	end    
})

local w = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
local j = game.Players.LocalPlayer.Character.Humanoid.JumpPower
local g = game.Workspace.Gravity

Tab:AddSlider({
	Name = "Walkspeed",
	Min = 0,
	Max = 200,
	Default = w,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(bool)	
	local Path = game.Players.LocalPlayer.Character.Humanoid
	local hook
	if bool then
		Path.WalkSpeed = bool
	end
end    
})

hook = hookmetamethod(game, "__index", function(self, ...)
    if self == Path and ... == "WalkSpeed" then
        return 16
    end
return hook(self,...)
end)    

Tab:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 400,
	Default = j,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(bool)
	if bool then
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = bool
        wait(0.1)
	end
end    
})

Tab:AddSlider({
	Name = "Client Sided Gravity Change",
	Min = 0,
	Max = 400,
	Default = g,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Gravity",
	Callback = function(bool)
	ClientGrav = bool
	if bool then
		game.Workspace.Gravity = bool
	end
end    
})

